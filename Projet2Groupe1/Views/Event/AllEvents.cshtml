@{
}

<!DOCTYPE html>
<html>
<head>
    <title>Image</title>
    <script type="text/javascript" src="/js/jquery-3.7.1.min.js"></script>
</head>
<body>

    @foreach (var eventItem in Model)
    {
        // retransforme en image
        String srcValue = "data:image.jpg;base64," + Convert.ToBase64String(eventItem.PhotoData) + "";
        <div>
            <a>@eventItem.EventName</a>
            <img id="imgViewer" width="200" height="200" style="border:1px solid #000000" src=@srcValue />
        </div>
    }



</body>
</html>